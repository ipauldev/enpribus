###############################
# Custom
###############################

d-i pkgsel/include string openssh-server puppetmaster
d-i pkgsel/update-policy select unattended-upgrades
tasksel	tasksel/skip-tasks	string standard
d-i finish-install/reboot_in_progress note
d-i partman-lvm/confirm_nooverwrite boolean true

d-i preseed/late_command string cp -a /cdrom/preseed/conf/enpribus-puppet.sh /target/tmp/preseed.sh && in-target chmod +x /tmp/preseed.sh && in-target /tmp/preseed.sh paul 'ssh-rsa  phassinger'
